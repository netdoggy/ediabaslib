<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0-android</TargetFramework>
    <SupportedOSPlatformVersion>21</SupportedOSPlatformVersion>
    <Nullable>disable</Nullable>
    <ImplicitUsings>disable</ImplicitUsings>
    <IncludeSourceRevisionInInformationalVersion>false</IncludeSourceRevisionInInformationalVersion>
    <DisableFastUpToDateCheck>true</DisableFastUpToDateCheck>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <NoWarn>1701;1702;CS0108;CS0114</NoWarn>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <NoWarn>1701;1702;CS0108;CS0114</NoWarn>
  </PropertyGroup>
  <Target Name="ListGeneratedFiles" AfterTargets="Build">
    <Message Text="ListGeneratedFiles" Importance="High" />
    <Exec Command="Setlocal EnableDelayedExpansion&#xD;&#xA;set &quot;testDir=$(ProjectDir)$(BaseIntermediateOutputPath)$(Configuration)\$(TargetFramework)\generated\src&quot;&#xD;&#xA;IF EXIST &quot;!testDir!&quot; DIR /B &quot;!testDir!&quot;" />
  </Target>
  <Target Name="PreBuild" BeforeTargets="PreBuildEvent">
    <Exec Command="Setlocal EnableDelayedExpansion&#xD;&#xA;set &quot;stampFile=$(ProjectDir)$(BaseIntermediateOutputPath)$(Configuration)\$(TargetFramework)\stamp\generator.stamp&quot;&#xD;&#xA;set &quot;mappingFile=$(ProjectDir)$(BaseIntermediateOutputPath)$(Configuration)\$(TargetFramework)\generated\__NamespaceMapping__.cs&quot;&#xD;&#xA;IF NOT EXIST &quot;!mappingFile!&quot; (&#xD;&#xA;  IF EXIST &quot;!stampFile!&quot; (&#xD;&#xA;    DEL /S &quot;!stampFile!&quot;&#xD;&#xA;  )&#xD;&#xA;)" />
  </Target>
</Project>